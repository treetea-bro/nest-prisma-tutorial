generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["prismaSchemaFolder"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

enum Language {
  KOREAN
  JAPANESE
  ENGLISH
  VIETNAMESE
}

model Department {
  id         Int      @id @default(autoincrement())
  name       String
  test_enum  Language
  create_at  DateTime @default(now())
  updated_at DateTime @default(now()) @updatedAt
}
